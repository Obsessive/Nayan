 <ScrollView>
 <StackLayout>
 	<Label text="{{MyProfile}}" id="patientprofilepage"/>
 	<Label text="{{FirstName}}" />
    <TextField text="" id="patientprofilefirstname" hint="{{FirstNamePlaceHolder}}" />

    <Label text="{{LastName}}" />
    <TextField text="" id="patientprofilelastname" hint="{{LastNamePlaceHolder}}" />

    <Label text="{{number}}" />
    <TextField text="" id="patientprofilephone" hint="{{numberPlaceHolder}}" />

    <Label text="{{Email}}" />
    <TextField text="" id="patientprofileemail" hint="{{EmailPlaceHolder}}" />

    <Label text="{{Referral}}" />
    <TextField editable="false" hint="{{ReferralPlaceHolder}}" text="" id="patientprofilecode"  />

    <Label text="{{Hospital}}" />
    <TextField text="" id="patientprofilehospital" hint="{{HospitalPlaceHolder}}" />

    <Label text="{{Address}}" />
    <TextField text="" id="patientprofileaddress" hint="{{AddressPlaceHolder}}" />

    <Button text="{{Save}}" class="introbutton" tap="{{ editAction }}" />
    <Button text="{{ResetProfile}}" class="introbutton" tap="{{ resetAction }}" />
 </StackLayout>
</ScrollView>
</Page>